app: Google Chrome
title: /Google Docs/
-

apply normal text: key(cmd-alt-0)
apply heading one: key(cmd-alt-1)
apply heading two: key(cmd-alt-2)
apply heading three: key(cmd-alt-3)

(numbered | number) list: key(cmd-shift-7)
bullet list: key(cmd-shift-8)

# Navigation
move to pop up: key(ctrl-cmd-e ctrl-cmd-p) 
next heading: key(ctrl-cmd-n ctrl-cmd-h)
last heading: key(ctrl-cmd-p ctrl-cmd-h)