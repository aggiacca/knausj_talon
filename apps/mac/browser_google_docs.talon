app: Firefox
app: Google Chrome
title: /Google Docs/
-

apply normal text: key(cmd-alt-0)
apply heading one: key(cmd-alt-1)
apply heading two: key(cmd-alt-2)
apply heading three: key(cmd-alt-3)

list (numbered | number): key(cmd-shift-7)
list bullet: key(cmd-shift-8)

# Navigation
move to pop up: key(ctrl-cmd-e ctrl-cmd-p) 
heading next: key(ctrl-cmd-n ctrl-cmd-h)
heading last: key(ctrl-cmd-p ctrl-cmd-h)